<!DOCTYPE html>

<html>
<head>
    <meta name="keywords" content="" />
    <meta name="description" content="" />
    <meta name="viewport" content="width=device-width" />
    <title>大中华区数据平台</title>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/templatemo_main.css")" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/site.css")" />
    <script src="~/Scripts/jquery-3.1.1.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/templatemo_script.js"></script>
    <script src="~/Scripts/FormSubmit.js"></script>
    <script src="~/Scripts/Fun.js"></script>
    <script src="~/Scripts/Check.js"></script>
</head>
<body>

    <div class="navbar navbar-inverse" role="navigation">
        <div class="navbar-header">
            <div class="logo"><h1>数据大盘 - 大中华区数据平台</h1></div>
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
        </div>
    </div>
    <div class="template-page-wrapper">
        <div class="navbar-collapse collapse templatemo-sidebar">
            <ul class="templatemo-sidebar-menu">
                <li>
                    <form class="navbar-form">
                        <input type="text" class="form-control" id="templatemo_search_box" placeholder="Search...">
                        <span class="btn btn-default">Go</span>
                    </form>
                </li>
                <li><a href="#"><i class="fa fa-home"></i>数据大盘</a></li>
                <li class="sub open active">
                    <a href="javascript:;">
                        <i class="fa fa-database"></i> 系统管理 <div class="pull-right"><span class="caret"></span></div>
                    </a>
                    <ul class="templatemo-submenu">
                        <li><a href="#">用户管理</a></li>
                        <li><a href="#">角色管理</a></li>
                        <li><a href="#">目录分配管理</a></li>
                        <li><a href="#">系统记录日志列表</a></li>
                        <li><a href="#">短信服务</a></li>
                    </ul>
                </li>
                <li><a href="data-visualization.html"><i class="fa fa-cubes"></i><span class="badge pull-right">9</span>实时业绩数据</a></li>
                <li><a href="maps.html"><i class="fa fa-map-marker"></i><span class="badge pull-right">42</span>数据分析</a></li>
                <li><a href="tables.html"><i class="fa fa-users"></i><span class="badge pull-right">NEW</span>团队/系统数据查询</a></li>
                <li><a href="preferences.html"><i class="fa fa-cog"></i>更换视角</a></li>
                <li><a href="javascript:;" data-toggle="modal" data-target="#confirmModal"><i class="fa fa-sign-out"></i>退出系统</a></li>
            </ul>
        </div><!--/.navbar-collapse -->
        <div class="templatemo-content-wrapper">
            <div class="templatemo-content">
                <ol class="breadcrumb">
                    <li><a href="index.html">系统管理</a></li>
                    <li><a href="#">用户管理</a></li>
                    <li class="active">新增用户</li>
                </ol>
                @RenderBody()
            </div>
        </div>

        <!-- Modal -->
        <div class="modal fade" id="confirmModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
            <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                        <h4 class="modal-title" id="myModalLabel">Are you sure you want to sign out?</h4>
                    </div>
                    <div class="modal-footer">
                        <a href="sign-in.html" class="btn btn-primary">Yes</a>
                        <button type="button" class="btn btn-default" data-dismiss="modal">No</button>
                    </div>
                </div>
            </div>
        </div>

        <!-- Modal -->
        <div class="modal fade" id="errModal" tabindex="-1" role="dialog" aria-labelledby="errModal" aria-hidden="true">
            <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                        <h4 class="modal-title" id="myModalLabel">提示信息：</h4>
                    </div>
                    <div class="modal-header">
                        <div class="row">
                            <div class="col-md-1 glyphicon glyphicon-alert" style="font-size: 30px"></div>
                            <div class="col-md-11" id="errMsg"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!-- Loading Modal -->
        <div class="modal fade" id="loading" data-backdrop="static" tabindex="-2" role="dialog" aria-labelledby="loading" aria-hidden="true">
            <div class="modal-body">
                <div class="loading">
                    <span></span>
                    <span></span>
                    <span></span>
                    <span></span>
                    <span></span>
                </div>
            </div>
        </div>

        <footer class="templatemo-footer">
            <div class="templatemo-copyright">
                <p>Copyright &copy; 2000 tiens group from <a href="http://www.tiens.com/" target="_blank">天狮集团</a></p>
            </div>
        </footer>
    </div>

</body>
</html>
